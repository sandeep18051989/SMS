@model SMS.Models.UserSettingsModel
@using EF.Services;
@{
    ViewBag.Title = "User Settings";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
<div class="row page-titles">
    <div class="col-md-5 align-self-center">
        <h3 class="text-themecolor">Years</h3>
    </div>
    <div class="col-md-7 align-self-center">
        @Html.Raw(Html.BuildBreadcrumbNavigation())
    </div>
</div>
<div class="row">
    <div class="col-md-12 mr-4">
        <br />
        @using (Html.BeginForm("Settings", "User", FormMethod.Post, new { enctype = "multipart/form-data", @class = "form-horizontal" }))
        {
            @Html.AntiForgeryToken()
            <div class="col-xs-12 col-md-12">
                <div class="input-group pull-right">
                    <input type="submit" value="Save Settings" class="btn btn-info" />
                </div>
            </div>
            <div class="clearfix"></div>
            <br />
        }
    </div>
</div>
<script type="text/javascript">
    $(document).ready(function() {
        $("li#settings ul").css("height", "");
    });
</script>