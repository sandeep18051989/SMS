<script type="text/javascript">
    $(document).ready(function () {
        if ($("#BusFacility").val()) {
            $("#BusFacility").attr('checked', true).val(true);
        }
        else {
            $("#BusFacility").attr('checked', false).val(false);
        }

        $("#BusFacility").change(function () {
            if ($(this).prop('checked')) {
                $(this).attr('checked', true).val(true);
                $("#bus-facility").show();
            }
            else {
                $(this).attr('checked', false).val(false);
                $("#bus-facility").hide();
            }
        });

        $("input[type=checkbox][data-check=Male]").change(function () {
            if ($(this).prop('checked')) {
                $(this).attr('checked', true).val(true);
                $("input[type=checkbox][data-check=FeMale]").attr('checked', false).val(false);
            }
            else {
                $(this).attr('checked', false).val(false);
                $("input[type=checkbox][data-check=FeMale]").attr('checked', true).val(true);
            }
        });

        $("input[type=checkbox][data-check=Male]").attr('checked', false).val(false);
        $("input[type=checkbox][data-check=FeMale]").attr('checked', false).val(false);

        $("input[type=checkbox][data-check=FeMale]").change(function () {
            if ($(this).prop('checked')) {
                $(this).attr('checked', true).val(true);
                $("input[type=checkbox][data-check=Male]").attr('checked', false).val(false);
            }
            else {
                $(this).attr('checked', false).val(false);
                $("input[type=checkbox][data-check=Male]").attr('checked', true).val(true);
            }
        });

        if ($("#IsActive").val()) {
            $("#IsActive").attr('checked', true).val(true);
        }
        else {
            $("#IsActive").attr('checked', false).val(false);
        }

        $("#IsActive").change(function () {
            if ($(this).prop('checked')) {
                $(this).attr('checked', true).val(true);
            }
            else {
                $(this).attr('checked', false).val(false);
            }
        });
    });
</script>
