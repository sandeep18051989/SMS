@{
    ViewBag.Title = "Page Not Found";
    Layout = "~/Views/Shared/_OneColumnLayout.cshtml";
}
<div class="row">
    <div id="register-form-wrapper" class="col-md-7 offset-md-2 mt-8">
        @using (Html.BeginForm("Register", "Account", new { Area = "" }, FormMethod.Post, new { enctype = "multipart/form-data", @class = "register-form" }))
        {
            <div class="card">
                <div class="card-block">
                    <div class="form-header green-gradient">
                        <h3><i class="fa fa-user"></i> Page Not Found</h3>
                    </div>
                    <div class="text-center">
                        Go Back To <a href="@Url.Action("Index", "Home", null)" class="btn btn-link"><i class='fa fa-home'></i><span><strong>Homepage</strong></span></a>
                    </div>
                </div>
            </div>
        }
    </div>
</div>