@using EF.Services.Http;
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" name="viewport">
    <!--favicon icon-->
    <link rel="icon" type="image/png" href="/Content/Images/logo.png">

    <meta name="description" content="SMS" />
    <meta name="author" content="Sandeep Kumar" />
    <title>@ViewBag.Title</title>

    @Styles.Render("~/Content/fo")
    @Styles.Render("~/Content/icons")
    @Styles.Render("~/Content/jqueryuicss")
    @Styles.Render("~/Content/boot")
    @Styles.Render("~/Content/Dev")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/Scripts/boot")
    <script type="text/javascript" src="~/Areas/Admin/Scripts/jquery.gritter.js"></script>
    @{
        var urlHelperService = EF.Core.ContextHelper.Current.Resolve<IUrlHelper>();
        var basePath = urlHelperService.GetLocation(false);
    }
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
        <script src="~/Scripts/html5shiv.js"></script>
        <script src="~/Scripts/respond.min.js"></script>
        <script src="~/Scripts/excanvas.min.js"></script>
    <![endif]-->
</head>
<body>
    <!-- Header Start -->
    <header id="tcd-header" class="tcd-header-1 menu-sticky">
        <div class="container">
            <div class="row">
                <div class="col-md-3 col-sm-9">
                    <div class="logo">
                        <a href="/"><img src="/Content/Images/logo.png" alt="Logo"></a>
                    </div>
                </div>
                <div class="col-md-9 col-sm-3">
                    <div class="mainmenu">
                        <ul class="nav-menu">
                            <li class="active"><a href="#tcd-banner">Home</a></li>
                            <li class=""><a href="#tcd-about">About</a></li>
                            <li class=""><a href="#tcd-services">Services</a></li>
                            <li class=""><a href="#tcd-portfolio">Works</a></li>
                            @*<li class=""><a href="#tcd-blog">Blog</a></li>*@
                            <li class=""><a href="#tcd-contact">Contact</a></li>
                        </ul>
                        @*<div class="hire-me-btn">
                            <a href="#">Hire Me</a>
                        </div>*@
                        <span>
                            <a href="#" id="nav-expander" class="nav-expander fixed">
                                <i class="fa fa-bars fa-lg white"></i>
                            </a>
                        </span>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <!-- Header End -->
    @RenderBody()
    <!-- Copyright Start -->
    <div class="tcd-copyright">
        <div class="container">
            <div class="row">
                @Html.Action("Copyright", "Developer")
            </div>
        </div>
    </div>
    <!-- Copyright End -->
    <!-- scrollUp start here-->
    <div id="toTop" style="display: none;">
        <i class="fa fa-rocket"></i>
    </div>
    <!-- Mobile menu start here-->
    <nav class="canvas-menu-area">
        <div class="close-btn"><span id="nav-close" class="text-center">x</span></div>
        <ul class="nav-menu">
            <li class="active"><a href="#tcd-banner">Home</a></li>
            <li><a href="#tcd-about">About</a></li>
            <li><a href="#tcd-services">What I Do</a></li>
            <li><a href="#tcd-testimonial">Testimonial</a></li>
            @*<li><a href="#tcd-blog">Blog</a></li>*@
            <li><a href="#tcd-contact">Contact</a></li>
        </ul>
    </nav>
    <!-- Mobile menu end here-->
    @Scripts.Render("~/Scripts/Dev")
    <script async="" src="https://www.google-analytics.com/analytics.js"></script>
</body>
</html>
