<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<meta name="description" content="">
	<meta name="author" content="">

	<title>@ViewBag.Title</title>
	@Scripts.Render("~/bundles/jquery")
	@Scripts.Render("~/bundles/jqueryui")
	@Scripts.Render("~/Scripts/boot")
	@Scripts.Render("~/Scripts/mdb")
	@Scripts.Render("~/Scripts/home")
	<script type="text/javascript" src="~/Scripts/tinymce/tinymce.min.js"></script>
	<script type="text/javascript" src="~/Scripts/tinymce/plugins/placeholder/plugin.min.js"></script>
	@*@Styles.Render("~/Content/fonts")*@
	@Styles.Render("~/Content/icons")
	@Styles.Render("~/Content/jqueryuicss")
	@Styles.Render("~/Content/boot")
	@Styles.Render("~/Content/mdb")
	@Styles.Render("~/Content/common")
</head>
<body>
	@Html.Partial("_menuLinks")
	@* Page Content *@
	<div id="main-wrapper" class="container main-wrapper-inner">
		@RenderBody()
	</div>
	<div class="clearfix"></div>
	<hr />
	<br />
	@Html.Partial("_Footer");
</body>
</html>
